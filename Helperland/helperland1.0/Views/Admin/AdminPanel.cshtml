@{
    Layout = null;
}





<!doctype html>

<html lang="en">

<head>


    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">


    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
          crossorigin="anonymous">

    <link rel="stylesheet" href="~/css/admin.css" />


    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">


    <title>Admin panal</title>


</head>

<body>

    <!-- nav-bar -->
    <section class="header">
        <nav class="navbar navbar-expand-md navbar-dark admin">
            <a class="navbar-brand" href="#"> helplander</a>

            <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                    data-bs-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false"
                    aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarNavAltMarkup">

                <div class="navbar-nav ms-auto">

                    <div class="user-icon nav-item">
                        <img src="/image/user.png" alt="...">
                        <span class="admin-name">@ViewBag.Name</span>
                        <a asp-controller="UserManagement" asp-action="logout"><img src="/image/logout.png" alt="" class="logout" ></a>
                    </div>

                </div>

            </div>

        </nav>


    </section>

    <!-- navbar-end -->
    <section class="main d-flex">
        <div class="row" style="height:100%">
            <div class="col-2  nav-col text-center">
                <div class="button tablinks" onclick="adminTab(event, 'AdminServiceReq')" id="servicertabbtn">
                    Service Request

                    @*<a data-bs-toggle="tab" data-bs-target="#AdminServiceReq"></a>*@

                </div>

                <div class="button tablinks" onclick="adminTab(event, 'Usermanagement')" id="usermanagementtabbtn">
                    User management
                    @* <a data-bs-toggle="tab" data-bs-target="#Usermanagement">User management</a>*@

                </div>




            </div>
            @await Html.PartialAsync("adminServiceReq")
            @await Html.PartialAsync("adminUserManagement")


        </div>
    </section>


    
    <section class="popupModel">









        <!-- Button trigger modal -->
        <button type="button" class="btn btn-primary d-none" data-bs-toggle="modal" id="AdminEditModelBtn" data-bs-target="#AdminEditModal">
            Launch demo modal
        </button>

        <!-- Modal -->
        <div class="modal fade" id="AdminEditModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered" style="max-width: 550px;">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalLabel">Edit Service Request</h5>
                        <button type="button" id="AdminEditModalClose" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                    <div class="modal-body" style="font-weight:500">
                        <div class="alert alert-danger d-none mt-3 d-flex justify-content-center" style="max-width:800px" id="AdminEditPopupAlert" role="alert">
                        </div>

                        <div class="row">
                            <div class="col-md-6">
                                <label for="AdminEditpopupDate" class="form-label">Date</label>
                                <input type="date" class="form-control" name="date" id="AdminEditPopupDate" placeholder="canlender">

                                @*min=@TomorrowDate value=@TomorrowDate*@
                            </div>
                            <div class="col-md-6">
                                <label class="form-label" for="AdminEditPopupTime">Time</label>
                                <select class="form-select" id="AdminEditPopupTime"
                                        aria-label="Default select example">
                                    <option selected value="07:00:00">7:00 </option>
                                    <option value="07:30:00">7:30 </option>
                                    <option value="08:00:00">8:00 </option>
                                    <option value="08:30:00">8:30 </option>
                                    <option value="09:00:00">9:00 </option>
                                    <option value="09:30:00">9:30 </option>
                                    <option value="10:00:00">10:00 </option>
                                    <option value="10:30:00">10:30 </option>
                                    <option value="11:00:00">11:00 </option>
                                    <option value="11:30:00">11:30 </option>
                                    <option value="12:00:00"> 12:00 </option>
                                    <option value="12:30:00">12:30 </option>
                                    <option value="13:00:00"> 13:00 </option>
                                    <option value="13:30:00">13:30 </option>
                                    <option value="14:00:00"> 14:00 </option>
                                    <option value="14:30:00">14:30 </option>
                                    <option value="15:00:00"> 15:00 </option>
                                    <option value="15:30:00">15:30 </option>
                                    <option value="16:00:00"> 16:00 </option>
                                    <option value="16:30:00">16:30 </option>
                                    <option value="17:00:00"> 17:00 </option>
                                    <option value="17:30:00">17:30 </option>
                                    <option value="18:00:00"> 18:00 </option>
                                </select>
                            </div>
                        </div>

                        <label class="form-label mt-4" style="font-weight:700"> Service Address</label>

                        <div class="row">
                            <div class="col-md-6">
                                <label for="AdminEditPopupStreet" class="form-label">Street Name</label>
                                <input type="text" class="form-control" name="Street" id="AdminEditPopupStreet">
                            </div>
                            <div class="col-md-6">
                                <label for="AdminEditPopupHouse" class="form-label">House No</label>
                                <input type="text" class="form-control" name="House" id="AdminEditPopupHouse">
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label for="AdminEditPopupPostalCode" class="form-label">PostalCode</label>
                                <input type="text" class="form-control" name="PostalCode"
                                       id="AdminEditPopupPostalCode">
                            </div>
                            <div class="col-md-6">
                                <label for="AdminEditPopupCity" class="form-label">City</label>
                                <input type="text" class="form-control" name="City" id="AdminEditPopupCity">

                            </div>
                        </div>
                        <label class="form-label mt-4" style="font-weight:700"> Invoice Address</label>

                        <div class="row">
                            <div class="col-md-6">
                                <label for="AdminEditPopupInvoiceStreet" class="form-label">Street Name</label>
                                <input type="text" class="form-control" name="Street"
                                       id="AdminEditPopupInvoiceStreet">
                            </div>
                            <div class="col-md-6">
                                <label for="AdminEditPopupInvoiceHouse" class="form-label">House No</label>
                                <input type="text" class="form-control" name="House"
                                       id="AdminEditPopupInvoiceHouse">
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <label for="AdminEditPopupInvoicePostalCode" class="form-label">PostalCode</label>
                                <input type="text" class="form-control" name="PostalCode"
                                       id="AdminEditPopupInvoicePostalCode">
                            </div>
                            <div class="col-md-6">
                                <label for="AdminEditPopupInvoiceCity" class="form-label">City</label>
                                <input type="text" class="form-control" name="City" id="AdminEditPopupInvoiceCity">

                            </div>
                        </div>


                        <!-- 1 col  -->
                        <div class="row">

                            <div class="row mt-4">
                                <label for="AdminEditPopupWhyEdit" class="form-label ">
                                    Why do you want to
                                    reSchedule Service req?
                                </label>
                                <textarea class="form-control ms-2" name="City" id="AdminEditPopupWhyEdit"
                                          placeholder="Why do you want to reSchedule Service req?"></textarea>
                            </div>
                            <div class="row mt-4">
                                <label for="AdminEditPopupWhyCall" class="form-label">
                                    Call Center EMP Notes
                                </label>
                                <textarea class="form-control ms-2" name="City" id="AdminEditPopupWhyCall"
                                          placeholder="Enter Notes"></textarea>
                            </div>
                            <div class="row mt-4 mb-2"><button type="button" id="AdminEditModalUpdateBtn" class=" ms-2 btn btn-primary ">Update</button></div>

                        </div>

                    </div>




                </div>
            </div>
        </div>




        	<!-- Button trigger modal -->
	<button type="button" class="btn btn-primary d-none" id="RefundModelBtn" data-bs-toggle="modal"
		data-bs-target="#RefundModel">

	</button>

	<!-- Modal -->
	<div class="modal fade" id="RefundModel" tabindex="-1">
		<div class="modal-dialog modal-dialog-centered" style="max-width: 550px;">
			<div class="modal-content">
				<div class="modal-header">
					<h4 class="modal-title w700" id="exampleModalLabel">Refund Amount</h4>
					<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
				</div>
				<div class="modal-body">
					<div class="alert alert-danger d-none  d-flex justify-content-center" style="max-width:800px" id="AdminRefundAlert" role="alert">
				</div>

					<div class="row ps-3">
						<div class="col-3 w500">

							<div class="row">Paid Amount </div>
							<span id="PaidAmount">5400</span>&euro;
						</div>
						<div class="col-4 w500">
							<div class="row">Refund Amount </div>
							<span id="RefundAmount">0</span>  &euro;
								</div>
						<div class="col-5 w500">
							<div class="row">In Balance Amount </div>
						<span id="BalanceAmount">5400</span> &euro;
						</div>

					</div>

					<div class="row mt-4">
						<div class="col-6">
							<label class="form-label w500">Amount</label>

							<div class="input-group">

								<input type="number" class="form-control" value="0" id="RefundPercentage">
								<span class="input-group-text" >Percentage</span>
							</div>
						
						</div>
						<div class="col-6">
							<label class="form-label w500">Calculate</label>

							<div class="input-group mb-3">
								<span class="input-group-text"> Calculate</span>
								<input type="text"  id="CalculateAmount" class="form-control" disabled>

							</div>
						</div>
					</div>

					<label class="form-label w500 mt-3">Why you want to refund amount?</label>
					<textarea name="" class="form-control" id="WhyRefund"
						placeholder="Why you want to refund amount?"></textarea>

					<label class="form-label w500 mt-3">Call Center EMP Notes</label>
					<textarea name="" class="form-control" id="CallNotes" placeholder="Enter Notes"></textarea>




					<div class="row my-3 px-3">
						<button class="btn btn-primary" style="background-color:#1FB6FF; border-radius: 0%; border-color: #1FB6FF;"
							type="button" id="RefundSubmit">Refund </button>

					</div>




				</div>
			</div>
		</div>

	</div>















        <button type="button" class="btn btn-primary d-none" id="complete" data-bs-toggle="modal" data-bs-target="#exampleModal">
            Launch demo modal
        </button>

        <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered" style="max-width: 300px;">
                <div class="modal-content">
                    <div class="modal-header">

                        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                    <div class="modal-body">
                        <h4 class="modal-title text-center" id="ModalLabel_SID"></h4>
                        <h5 id="Model_SID"></h5>
                    </div>

                </div>
            </div>
        </div>








    </section>




    <!-- script -->

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.5.1.js"></script>
    <script type="text/javascript" src="https://unpkg.com/xlsx@0.15.1/dist/xlsx.full.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/v/dt/dt-1.11.3/r-2.2.9/rg-1.1.4/datatables.min.js"></script>
    <script src="https://cdn.datatables.net/responsive/2.2.9/js/dataTables.responsive.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.1.0/js/dataTables.buttons.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.1.0/js/buttons.html5.min.js"></script>
    <script src="https://cdn.datatables.net/1.11.5/js/jquery.dataTables.min.js"></script>
    <script src="/js/admin.js"></script>

</body>

</html>
